{
    "version": 2,
    "builds": [
      {
        "src": "*.py",
        "use": "@vercel/python"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/$1.py"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  